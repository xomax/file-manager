{
  "name": "xomax/file-manager",
  "description": "File manager",
  "authors": [
    {
      "name": "Tom Hnatovsky",
      "email": "hnatovsky@xswd.cz"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/xomax/file-icon-generator.git"
    },
    {
      "type": "composer",
      "url": "https://xs.repo.si/composer"
    }
  ],
  "require": {
    "symfony/filesystem": "^4.2",
    "symfony/finder": "^4.2",
    "kumatch/filename-normalizer": "^1.0",
    "wapmorgan/file-type-detector": "^1.0",
    "siriusphp/upload": "^3.0",
    "intervention/image": "^2.3",
    "xomax/file-icon-generator": "0.*",
    "mattketmo/camel": "^1.1",
    "xs/file-system-helper": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "xomax\\FileManager\\": "src/FileManager/"
    }
  },
  "require-dev": {
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
